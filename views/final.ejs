<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dawatee Khazaane</title>

    <link
      rel="stylesheet"
      href="https://unpkg.com/swiper/swiper-bundle.min.css"
    />

    <!-- font awesome cdn link  -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
    />

    <!-- custom css file link  -->
    <link rel="stylesheet" href="final/final.css" />
  </head>
  <body>
    <h1>Hello World</h1>
    <!-- header section starts      -->

    <header>
      <a href="#" class="logo"
        ><i class="fas fa-utensils"></i> Dawatee Khazaane</a
      >

      <nav class="navbar">
        <a class="active" href="#home">home</a>
        <a href="#Explore">Explore</a>
        <a href="#review">review</a>
        <a href="#about">about</a>
      </nav>

      <div class="right-part">
        <div class="search">
          <form action="/search" method="post">
            <div class="input-box">
              <input
                type="text"
                name="searchData"
                placeholder="Search"
                autocomplete="off"
              />
              <button type="submit"><i class="fas fa-search"></i></button>
            </div>
          </form>
        </div>

        <div class="icons">
          <i class="fas fa-bars" id="menu-bars"></i>

          <form action="/newThelewala" method="post" class="add-new">
            <input type="submit" class="new-thelewala" value="+" />
          </form>
          <!-- <a href="#" class="fas fa-shopping-cart"></a> -->
        </div>
      </div>
    </header>

    <!-- header section ends-->

    <!-- search form  -->

    <form action="" id="search-form">
      <input
        type="search"
        placeholder="search here..."
        name=""
        id="search-box"
      />
      <label for="search-box" class="fas fa-search"></label>
      <i class="fas fa-times" id="close"></i>
    </form>

    <!-- home section starts  -->

    <section class="home" id="home">
      <div class="home-slider">
        <div class="wrapper">
          <div class="slide slider-1">
            <div class="content">
              <span></span>
              <h3 style="font-size: 4rem;">One Plate Food Finder</h3>
              <p style="text-transform: none;">
                One stop solution to all your desi cravings. From Samosa's to
                Pani-puris, we've got you covered. What are you waiting for?
              </p>
              <a href="#Explore" class="btn">Explore Now</a>
            </div>
            <div class="image">
              <button class="left-btn change-button"><</button>
              <img
                src="index/img1.avif"
                class="home-img"
                height="200px"
                alt=""
              />
              <button class="right-btn change-button">></button>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- home section ends -->

    <!-- dishes section starts  -->

    <!-- <section class="dishes" id="dishes">
      <h3 class="sub-heading">our dishes</h3>
      <h1 class="heading">popular dishes</h1>

